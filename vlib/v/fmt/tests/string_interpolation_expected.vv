struct Aa {
	xy int
}

struct Bb {
	a Aa
}

struct Cc {
	a []Aa
}

fn main() {
	st := Bb{Aa{5}}
	ar := Cc{[Aa{3}, Aa{-4}, Aa{12}]}
	aa := Aa{-13}
	z := -14.75
	println('$st.a.xy ${ar.a[2].xy} $aa.xy $z')
	println('$st.a.xy${ar.a[2].xy}$aa.xy$z')
	println('${st.a.xy}ya ${ar.a[2].xy}X2 ${aa.xy}.b ${z}3')
	println('${z:-5} ${z:+5.3} ${z:+09.3f} ${z:-7.2} ${z:+09} ${z:08.3f}')
}
