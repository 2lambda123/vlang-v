fn main() {
	a1 := [1, 2, 3]
	for mut j in a1 {
		unsafe { *j *= 2 }
	}
	a2 := [1, 2, 3]!
	for mut j in a2 {
		unsafe { *j *= 2 }
	}
	m := map{'aa': 1, 'bb': 2}
	for _, mut j in m {
		unsafe { *j *= 2 }
	}
	for mut j in [1, 2, 3] {
		unsafe { *j *= 2 }
	}
	for mut j in [1, 2, 3]! {
		unsafe { *j *= 2 }
	}
	for _, mut j in map{'aa': 1, 'bb': 2} {
		unsafe { *j *= 2 }
	}
}
