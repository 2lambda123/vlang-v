#!/usr/bin/env expect
set send_human {.1 .3 1 .05 2}

spawn [lindex $argv 0]
set test_file [lindex $argv 1]

expect -ex "Break on \[main\] comptime_variant_int in ${test_file}:7"
expect -ex "${test_file}:7 vdbg> " { send -h "p a\n" } timeout { exit 1 }
expect -ex "a = 0 (int)" { send -h "q\n" } timeout { exit 1 }
expect eof
