fn test_unicode_characters() {
	unicode := '😀😆😎💻🌎'
	println(unicode)
}

fn test_escape_codes() {
	star1 := '\xe2\x98\x85'
	println(star1)
	star2 := '\u2605'
	println(star2)

	aaa := '\x61\141a'
	println(aaa)

	tab_newln := '##\t#\n###\t#'
	println(tab_newln)
}

fn test_raw_string() {
	raw := r'hello\tworld\n'
	println(raw)
}

fn test_compare_strings() {
	assert 'abcde' == 'abcde'
	assert 'abcde' != 'abcdef'
	assert 'abcde' != '12345'
	assert 'abcde' != 'abcdd'

	if 'abcde' != 'abcde' {
		assert false
	}

	if 'abcde' == '@#$&*' {
		assert false
	}
}

fn main() {
	test_unicode_characters()
	test_escape_codes()
	test_raw_string()
	test_compare_strings()
}